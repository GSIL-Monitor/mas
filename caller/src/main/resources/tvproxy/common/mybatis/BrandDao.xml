<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.letv.mas.caller.iptv.tvproxy.common.model.dao.db.BrandDao">
    
    <select id="getBrandByCode" resultType="com.letv.mas.caller.iptv.tvproxy.common.model.dao.db.table.Brand">
        select * from terminal.BRAND where hardware_code = #{code}
    </select>
     
    <select id="getBrandList" resultType="com.letv.mas.caller.iptv.tvproxy.common.model.dao.db.table.Brand">
        select * from terminal.BRAND
    </select>
</mapper>