<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.letv.mas.caller.iptv.tvproxy.common.model.dao.db.DeskConfigMysqlDao">

    <select id="getList"
        resultType="com.letv.mas.caller.iptv.tvproxy.common.model.dao.db.table.DeskConfigMysqlTable">
        select * from iptv.ITV_DESK_CONFIG where model=#{model} 
        ORDER BY page , ordernu ASC ;
    </select>
</mapper>
